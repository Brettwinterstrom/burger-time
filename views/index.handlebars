<h1>Burger Time</h1>

<p>Time To Eat!</p>

<div>un eaten burgers</div>
<ul>
    {{#each burger}}
    {{#unless this.devoured}}
    <li>
        <p><span class="label">ID:</span> {{this.id}}</p>
        <p><span class="label">Burger:</span> {{this.burger_name}}</p>
    </li>
    {{/unless}}
    {{/each}}
</ul>

<div>Eaten burgers</div>
<ul>
    {{#each burger}}
    {{#if this.devoured}}
    <li>
        <p><span class="label">ID:</span> {{this.id}}</p>
        <p><span class="label">Burger:</span> {{this.burger_name}}</p>
    </li>
    {{/if}}
    {{/each}}
</ul>